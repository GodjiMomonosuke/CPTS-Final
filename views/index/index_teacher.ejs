                                borderColor: '#2F3C3D',
                                fill: true,
                                backgroundColor: '#bd7b677e',
                            }]
                        };
                        // config 
                        const config = {
                            type: 'radar',
                            data,
                            options: {}
                            };
                        // render init block
                        const myChart = new Chart(
                            document.getElementById('myChart'),
                            config
                        );
                        
                        // Instantly assign Chart.js version
